    "    credit_score FLOAT NOT NULL,\n",
    "    stocks_owned JSONB NOT NULL,\n",
    "    annual_income FLOAT NOT NULL,\n",
    "    recent_purchases JSONB NOT NULL,\n",
    "    extra_comments TEXT,\n",
    "    feedback TEXT,\n",
    "    experience INT NOT NULL\n",
    ");\n",
    "\"\"\"\n",
    "\n",
    "# Execute the SQL query\n",
    "cur.execute(create_table_query)\n",
